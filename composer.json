{
    "name": "BAWES/plug.to",
    "description": "Never miss an Instagram Comment!",
    "keywords": ["instagram", "crm", "plug", "customer service"],
    "homepage": "http://www.plug.to/",
    "type": "project",
    "minimum-stability": "stable",
    "require": {
        "php": ">=5.4.0",
        "yiisoft/yii2": ">=2.0.6",
        "yiisoft/yii2-bootstrap": "*",
        "yiisoft/yii2-swiftmailer": "*",
        "yiisoft/yii2-imagine": "~2.0.0",
        "yiisoft/yii2-httpclient": "dev-master",
        "understeam/yii2-slack": "*",
        "openbuildings/swiftmailer-css-inliner": "*",
        "stichoza/google-translate-php": "~3.2",
        "yiisoft/yii2-authclient": "*",
        "dektrium/yii2-user": "0.9.*@dev"
    },
    "require-dev": {
        "yiisoft/yii2-codeception": "*",
        "yiisoft/yii2-debug": "*",
        "yiisoft/yii2-gii": "*",
        "yiisoft/yii2-faker": "*"
    },
    "config": {
        "process-timeout": 1800
    },
    "extra": {
        "asset-installer-paths": {
            "npm-asset-library": "vendor/npm",
            "bower-asset-library": "vendor/bower"
        }
    }
}
