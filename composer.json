{
    "name": "BAWES/plugn",
    "description": "Never miss an Instagram Comment!",
    "version": "2.0.2",
    "keywords": ["instagram", "crm", "plug", "customer service"],
    "homepage": "http://www.plugn.io/",
    "type": "project",
    "minimum-stability": "stable",
    "require": {
        "php": ">=5.4.0",
        "yiisoft/yii2": ">=2.0.6",
        "yiisoft/yii2-bootstrap": "*",
        "yiisoft/yii2-swiftmailer": "*",
        "yiisoft/yii2-imagine": "~2.0.0",
        "yiisoft/yii2-httpclient": "dev-master",
        "yiisoft/yii2-redis": "~2.0.0",
        "yiisoft/yii2-shell": "~2.0.0",
        "understeam/yii2-slack": "v0.3.0",
        "openbuildings/swiftmailer-css-inliner": "*",
        "stichoza/google-translate-php": "~3.2",
        "yiisoft/yii2-authclient": "2.0.5",
        "kotchuprik/yii2-instagram-authclient": "v1.0",
        "2checkout/2checkout-php": "^0.3.1",
        "google/apiclient": "^2.1"
    },
    "require-dev": {
        "yiisoft/yii2-codeception": "*",
        "yiisoft/yii2-debug": "*",
        "yiisoft/yii2-gii": "*",
        "yiisoft/yii2-faker": "*"
    },
    "config": {
        "process-timeout": 1800
    },
    "extra": {
        "asset-installer-paths": {
            "npm-asset-library": "vendor/npm",
            "bower-asset-library": "vendor/bower"
        }
    }
}
